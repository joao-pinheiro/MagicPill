{
  "name": "magicpill/resource",
  "description": "MagicPill resource loader",
  "license": "BSD-2-Clause",
  "keywords": [
    "magicpill",
    "resource"
  ],
  "autoload": {
    "psr-4": {
      "MagicPill\\Mixin\\": ""
    }
  },
  "target-dir": "MagicPill/Resource",
  "require": {
    "php": ">=5.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev",
      "dev-develop": "1.0-dev"
    }
  }
}